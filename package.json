{
  "name": "newsfeed-app",
  "version": "1.0.0",
  "description": "An application that allows users to view all headlines from different news sources and link to the original news on those sources.",
  "main": "src/app/app.js",
  "scripts": {
    "build": "./node_modules/.bin/webpack --output-public-path=/public/",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/.bin/coveralls",
    "coverage": "./node_modules/.bin/nyc report --reporter=text-lcov | ./node_modules/.bin/coveralls",
    "dev": "webpack-dev-server --content-base src  --output-public-path=/public/ --history-api-fallback",
    "test": "mocha --require ignore-styles tests/.setup.js tests/**/*[sS]pec.js",
    "test:nyc": "./node_modules/.bin/nyc --reporter=html --reporter=text mocha tests/.setup.js tests/*[sS]pec.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andela-iukwuoma/newsfeed.git"
  },
  "keywords": [
    "rss",
    "feed",
    "react",
    "webpack"
  ],
  "author": "Ignatius Ukwuoma",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/andela-iukwuoma/newsfeed/issues"
  },
  "homepage": "https://github.com/andela-iukwuoma/newsfeed#readme",
  "dependencies": {
    "babel-core": "^6.24.1",
    "babel-loader": "^6.4.1",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "dotenv": "^4.0.0",
    "dotenv-webpack": "^1.4.3",
    "events": "^1.1.1",
    "express": "^4.15.2",
    "firebase": "^3.9.0",
    "firebaseui": "^1.0.1",
    "flux": "^3.1.2",
    "history": "^4.6.1",
    "json-loader": "^0.5.4",
    "path": "^0.12.7",
    "prop-types": "^15.5.8",
    "react": "^15.5.3",
    "react-dom": "^15.5.3",
    "react-router": "^3.0.5",
    "react-select": "^1.0.0-rc.3",
    "superagent": "^3.5.2",
    "webpack": "^2.3.3",
    "webpack-dev-server": "^2.4.2"
  },
  "devDependencies": {
    "babel-plugin-react-html-attrs": "^2.0.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-airbnb": "^2.2.3",
