    "chai": "^3.5.0",
    "chai-enzyme": "^0.6.1",
    "cheerio": "^0.22.0",
    "cover": "^0.2.9",
    "coveralls": "^2.13.1",
    "css-loader": "^0.28.0",
    "enzyme": "^2.8.2",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-config-airbnb-base": "^11.1.3",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "ignore-styles": "^5.0.1",
    "jsdom": "^9.12.0",
    "mocha": "^3.3.0",
    "mocha-lcov-reporter": "^1.3.0",
    "node-sass": "^4.5.2",
    "nyc": "^10.3.0",
